{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"]
    }
  },
  "include": [
    "app/**/*.ts",
    "api/**/*.ts",
    "app/**/*.tsx",
    "components/**/*.tsx",
    "utils/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
